#dependencies
sudo yum install qjackctl PyQt4 pyliblo gcc alsa-lib-devel liblo-devel libsndfile-devel gcc-c++ audacity qt-devel SDL-devel jack-audio-connection-kit-devel

#Now run:
#make clean
#make

#Or to compile with debug symbols:
#make debug

#followed by this command as root
#make install

#or this command if packaging
#make DESTDIR=/your/packaging/dir install
